{
	"name": "RemoveIFrame",
	"version": "0.2",
	"manifest_version": 2,
	"description": "remove the inline frame",
	"icons": {
    	"48" : "image/icon48.png",
    	"128" : "image/icon128.png"
  	},
	"content_scripts": [{
		"matches": ["http://*/*", "https://*/*"],
		"js": ["js/script.js"],
		"run_at": "document_start"
	}],
	"browser_action" : {
    	"default_icon" : "image/icon.png",
    	"default_title" : "RemoveIFrame",
    	"default_popup" : "popup.html"
	},
	"permissions": [
		"storage",
		"tabs"
	]
}
